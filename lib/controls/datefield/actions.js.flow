// @flow

import type {SetValue, SetCurrent, Previous, Next, Begin, Cancel, Choose, Clear} from "./types";
declare export function begin(): {type: "BEGIN"};
declare export function cancel(): {type: "CANCEL"};
declare export function choose(value: string): {type: "CHOOSE", value: string};
declare export function clear(): {type: "CLEAR"};
declare export function next(): {type: "NEXT"};
declare export function previous(): {type: "PREVIOUS"};
declare export function setCurrent(month: number, year: number): {month: number, type: "SET_CURRENT", year: number};
declare export function setValue(value: string): {type: "SET_VALUE", value: string};
